<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>CUSTOM-generate-sources</actionName>
            <displayName>generate-sources</displayName>
            <goals>
                <goal>generate-sources</goal>
            </goals>
        </action>
        <action>
            <actionName>CUSTOM-findbugs-gui</actionName>
            <displayName>findbugs-gui</displayName>
            <goals>
                <goal>findbugs:gui</goal>
            </goals>
        </action>
        <action>
            <actionName>run.single.main</actionName>
            <packagings>
                <packaging>*</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.6.0:exec</goal>
            </goals>
            <properties>
                <exec.args>-classpath %classpath ${packageClassName}</exec.args>
                <exec.executable>java</exec.executable>
                <exec.classpathScope>${classPathScope}</exec.classpathScope>
            </properties>
        </action>
    </actions>
